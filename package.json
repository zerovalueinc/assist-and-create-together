  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:build": "tsc -p tsconfig.backend.json",
    "server:start": "node dist/server/index.js",
    "server:dev": "npm run server:build && node dist/server/index.js",
    "server:watch": "nodemon --watch server --watch agents --watch core --watch utils --watch webhooks --exec \"npm run server:build && node dist/server/index.js\"",
    "backend:install": "npm install --prefix ./backend",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "build:full": "npm run server:build && npm run build",
    "start:full": "npm run server:start"
  },
